<h4 class="text-center text-dark">Ingrese sus datos</h4>
<hr>
<br>
<%= form_for(resource, as: resource_name, url: session_path(resource_name), html_options: {class: "user"}) do |f| %>
  <div class="form-group">
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control form-control-user" %>
  </div>

  <div class="form-group">
    <%= f.password_field :password, autocomplete: "current-password", class: "form-control form-control-user" %>
  </div>

 

  <div class="form-group">
    <%= f.submit "Iniciar Sesion", class: "btn btn-primary btn-user btn-block" %>
  </div>
<% end %>
<hr>
<%= render "devise/shared/links" %>
